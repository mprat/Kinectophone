<Window x:Class="Kinectophone.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="700" Width="700" Loaded="Window_Loaded">
    <Grid Name="kinectGrid" Height="650" Width="650">
        <Image Height="550" HorizontalAlignment="Left" Margin="34,42,0,0" Name="kinectColorOut" Stretch="Fill" VerticalAlignment="Top" Width="550" />
        <Canvas Height="550" HorizontalAlignment="Left" Margin="34,42,0,0" Name="canvas1" VerticalAlignment="Top" Width="550">
            <Ellipse Canvas.Left="6" Canvas.Top="6" Height="20" Name="headEllipse" Stroke="Aqua" Width="20" Fill="Aqua"/>
        </Canvas>
    </Grid>
</Window>
